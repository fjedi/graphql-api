{
  "name": "@fjedi/graphql-api",
  "version": "11.0.0",
  "description": "GraphQL API with built-in REST API support",
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "files": [
    "lib/"
  ],
  "private": false,
  "scripts": {
    "test": "jest --detectOpenHandles --forceExit",
    "test:watch": "jest --watchAll",
    "build": "npm run clean && tsc -p tsconfig.json",
    "clean": "rimraf lib",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "start": "tsc -w",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepare": "npm run build && husky install",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fjedi/graphql-api.git"
  },
  "keywords": [
    "fjedi",
    "graphql",
    "rest",
    "apollo"
  ],
  "author": "Alexander Radyushin <r.alex.andr@ya.ru> (https://fjedi.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fjedi/graphql-api/issues"
  },
  "homepage": "https://github.com/fjedi/graphql-api#readme",
  "dependencies": {
    "@fjedi/database-client": "4.6.6",
    "@fjedi/errors": "1.1.4",
    "@fjedi/jwt": "1.0.4",
    "@fjedi/logger": "1.46.3",
    "@fjedi/redis-client": "1.167.1",
    "@fjedi/rest-api": "3.5.4",
    "@graphql-tools/delegate": "7.1.5",
    "@graphql-tools/schema": "7.1.5",
    "@types/component-emitter": "1.2.11",
    "@types/git-rev-sync": "2.0.0",
    "@types/graphql-upload": "16.0.0",
    "@types/ip": "1.1.0",
    "@types/useragent": "2.3.1",
    "@types/ws": "8.5.3",
    "@types/yup": "0.29.14",
    "apollo-server-cache-redis": "3.3.1",
    "apollo-server-core": "3.11.0",
    "apollo-server-koa": "3.10.3",
    "apollo-server-plugin-response-cache": "3.7.1",
    "bluebird": "3.7.2",
    "cookie": "0.5.0",
    "git-rev-sync": "3.0.2",
    "graphql": "16.6.0",
    "graphql-jit": "0.8.0",
    "graphql-redis-subscriptions": "2.6.0",
    "graphql-scalar": "0.0.11",
    "graphql-scalars": "1.20.4",
    "graphql-subscriptions": "2.0.0",
    "graphql-upload": "16.0.2",
    "graphql-ws": "5.12.0",
    "invariant": "2.2.4",
    "ip": "1.1.8",
    "json-buffer": "3.0.1",
    "lodash": "4.17.21",
    "mysql2": "2.3.3",
    "tiny-lru": "8.0.2",
    "useragent": "2.3.0",
    "ws": "8.9.0"
  },
  "devDependencies": {
    "@types/cookie": "0.5.1",
    "@types/jest": "28.1.5",
    "@types/lodash": "4.14.182",
    "@types/node": "17.0.41",
    "@typescript-eslint/eslint-plugin": "5.43.0",
    "@typescript-eslint/parser": "5.43.0",
    "eslint": "8.27.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-prettier": "4.2.1",
    "husky": "8.0.1",
    "jest": "28.1.3",
    "lint-staged": "13.0.3",
    "prettier": "2.7.1",
    "rimraf": "3.0.2",
    "ts-jest": "28.0.5",
    "typescript": "4.9.5"
  },
  "lint-staged": {
    "*.(js|ts)": [
      "npm run lint:fix",
      "git add"
    ]
  },
  "engines": {
    "node": ">=16",
    "npm": ">=7"
  },
  "overrides": {
    "graphql@>12 <20": "$graphql",
    "graphql-subscriptions@1": "$graphql-subscriptions"
  },
  "resolutions": {
    "graphql": "15.8.0"
  }
}
