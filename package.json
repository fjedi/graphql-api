{
  "name": "@fjedi/graphql-api",
  "version": "11.3.3",
  "description": "GraphQL API with built-in REST API support",
  "main": "./lib/index.js",
  "files": [
    "lib/"
  ],
  "private": false,
  "scripts": {
    "test": "jest --detectOpenHandles --forceExit",
    "test:watch": "jest --watchAll",
    "build": "npm run clean && tsc -p tsconfig.json",
    "clean": "rimraf lib",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "start": "tsc -w",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepare": "npm run build && husky install",
    "prepublishOnly": "npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fjedi/graphql-api.git"
  },
  "keywords": [
    "fjedi",
    "graphql",
    "rest",
    "apollo"
  ],
  "author": "Alexander Radyushin <r.alex.andr@ya.ru> (https://fjedi.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fjedi/graphql-api/issues"
  },
  "homepage": "https://github.com/fjedi/graphql-api#readme",
  "dependencies": {
    "@fjedi/errors": "1.1.4",
    "@fjedi/jwt": "1.0.4",
    "@fjedi/logger": "1.46.3",
    "@fjedi/redis-client": "1.167.1",
    "@fjedi/rest-api": "5.1.1",
    "@graphql-tools/delegate": "10.0.3",
    "@graphql-tools/schema": "10.0.0",
    "@types/component-emitter": "1.2.12",
    "@types/git-rev-sync": "2.0.0",
    "@types/graphql-upload": "16.0.2",
    "@types/ip": "1.1.0",
    "@types/useragent": "2.3.2",
    "@types/ws": "8.5.5",
    "@types/yup": "0.29.14",
    "apollo-server-cache-redis": "3.3.1",
    "apollo-server-core": "3.12.0",
    "apollo-server-koa": "3.12.0",
    "apollo-server-plugin-response-cache": "3.8.2",
    "bluebird": "3.7.2",
    "cookie": "0.5.0",
    "fs-capacitor": "8.0.0",
    "git-rev-sync": "3.0.2",
    "graphql": "16.8.1",
    "graphql-jit": "0.8.4",
    "graphql-redis-subscriptions": "2.6.0",
    "graphql-scalar": "0.1.0",
    "graphql-scalars": "1.22.2",
    "graphql-subscriptions": "2.0.0",
    "graphql-upload": "16.0.2",
    "graphql-ws": "5.14.2",
    "invariant": "2.2.4",
    "ip": "1.1.8",
    "json-buffer": "3.0.1",
    "lodash": "4.17.21",
    "mysql2": "3.6.1",
    "tiny-lru": "11.1.2",
    "useragent": "2.3.0",
    "ws": "8.14.2"
  },
  "devDependencies": {
    "@types/cookie": "0.5.2",
    "@types/jest": "29.5.5",
    "@types/lodash": "4.14.198",
    "@types/node": "20.6.3",
    "@typescript-eslint/eslint-plugin": "6.9.0",
    "@typescript-eslint/parser": "6.9.0",
    "eslint": "8.49.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "17.1.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-prettier": "5.0.0",
    "husky": "8.0.3",
    "jest": "29.7.0",
    "lint-staged": "14.0.1",
    "prettier": "3.0.3",
    "rimraf": "5.0.5",
    "ts-jest": "29.1.1",
    "typescript": "5.2.2"
  },
  "peerDependencies": {
    "@fjedi/database-client": "5.3.7"
  },
  "lint-staged": {
    "*.(js|ts)": [
      "npm run lint:fix",
      "git add"
    ]
  },
  "engines": {
    "node": ">=18",
    "npm": ">=8"
  },
  "overrides": {
    "graphql-subscriptions@1": "$graphql-subscriptions"
  }
}
