{
  "name": "@fjedi/graphql-api",
  "version": "9.1.3",
  "description": "GraphQL API with built-in REST API support",
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "files": [
    "lib/"
  ],
  "private": false,
  "scripts": {
    "test": "jest --detectOpenHandles --forceExit",
    "test:watch": "jest --watchAll",
    "build": "npm run clean && tsc -p tsconfig.json && tsc -p tsconfig-cjs.json",
    "clean": "rimraf lib",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "start": "tsc -w",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepare": "npm run build && husky install",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fjedi/graphql-api.git"
  },
  "keywords": [
    "fjedi",
    "graphql",
    "rest",
    "apollo"
  ],
  "author": "Alexander Radyushin <r.alex.andr@ya.ru> (https://fjedi.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fjedi/graphql-api/issues"
  },
  "homepage": "https://github.com/fjedi/graphql-api#readme",
  "dependencies": {
    "@fjedi/database-client": "4.2.3",
    "@fjedi/errors": "1.1.4",
    "@fjedi/graphql-resolvers": "2.35.9",
    "@fjedi/graphql-shield": "0.0.4",
    "@fjedi/jwt": "1.0.4",
    "@fjedi/logger": "1.46.3",
    "@fjedi/redis-client": "1.166.0",
    "@fjedi/rest-api": "2.3.5",
    "@graphql-tools/delegate": "8.2.2",
    "@graphql-tools/schema": "8.2.0",
    "@types/component-emitter": "1.2.10",
    "@types/git-rev-sync": "2.0.0",
    "@types/ip": "1.1.0",
    "@types/useragent": "2.3.1",
    "@types/yup": "0.29.13",
    "apollo-server-cache-redis": "1.5.0",
    "apollo-server-koa": "2.25.2",
    "apollo-server-plugin-response-cache": "0.9.0",
    "bluebird": "3.7.2",
    "cookie": "0.4.1",
    "git-rev-sync": "3.0.1",
    "graphql": "15.6.1",
    "graphql-middleware": "6.1.7",
    "graphql-redis-subscriptions": "2.4.0",
    "invariant": "2.2.4",
    "ip": "1.1.5",
    "json-buffer": "3.0.1",
    "lodash": "4.17.21",
    "mysql2": "2.3.0",
    "useragent": "2.3.0"
  },
  "devDependencies": {
    "@types/cookie": "0.4.1",
    "@types/jest": "27.0.2",
    "@types/lodash": "4.14.175",
    "@types/node": "14.17.20",
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "@typescript-eslint/parser": "4.33.0",
    "eslint": "7.32.0",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-config-airbnb-typescript": "14.0.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.24.2",
    "eslint-plugin-prettier": "4.0.0",
    "husky": "7.0.2",
    "jest": "27.2.4",
    "lint-staged": "11.2.0",
    "prettier": "2.4.1",
    "rimraf": "3.0.2",
    "ts-jest": "27.0.5",
    "typescript": "4.4.3"
  },
  "lint-staged": {
    "*.(js|ts)": [
      "npm run lint:fix",
      "git add"
    ]
  },
  "engines": {
    "node": ">=15",
    "npm": ">=7"
  }
}
